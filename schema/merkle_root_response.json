{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MerkleRootResponse",
  "type": "object",
  "required": [
    "merkle_root",
    "stage"
  ],
  "properties": {
    "merkle_root": {
      "description": "MerkleRoot is hex-encoded merkle root.",
      "type": "string"
    },
    "stage": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    }
  }
}
