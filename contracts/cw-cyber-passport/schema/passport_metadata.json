{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PassportMetadata",
  "type": "object",
  "required": [
    "avatar",
    "nickname"
  ],
  "properties": {
    "addresses": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "avatar": {
      "type": "string"
    },
    "nickname": {
      "type": "string"
    }
  }
}
